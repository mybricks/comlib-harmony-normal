{
  "title": "阿里云OSS上传",
  "namespace": "mybricks.harmony._aliyunOssUpload",
  "author": "mybricks-team",
  "author_name": "板砖团队",
  "version": "1.0.0",
  "description": "使用阿里云OSS进行文件上传",
  "icon": "./icon.svg",
  "data": "./data.json",
  "editors": "./editors.tsx",
  "runtime": "./runtime.tsx",
  "rtType": "js",
  "inputs": [
    {
      "id": "upload",
      "title": "上传参数/路径",
      "schema": {
        "type": "object",
        "properties": {
          "filePath": {
            "title": "文件路径",
            "type": "string"
          },
          "accessKeyId": {
            "title": "AccessKeyId",
            "type": "string",
            "description": "STS临时访问凭证的Access Key ID"
          },
          "accessKeySecret": {
            "title": "AccessKeySecret",
            "type": "string",
            "description": "STS临时访问凭证的Access Key Secret"
          },
          "securityToken": {
            "title": "SecurityToken",
            "type": "string",
            "description": "STS临时访问凭证的Security Token"
          },
          "region": {
            "title": "Region",
            "type": "string",
            "description": "填写Bucket所在地域。以华东1（杭州）为例，Region填写为oss-cn-hangzhou"
          },
          "bucket": {
            "title": "Bucket",
            "type": "string",
            "description": "指定要操作的Bucket名称，请替换为您实际使用的Bucket名称"
          }
        }
      }
    }
  ],
  "outputs": [
    {
      "id": "onSuccess",
      "title": "上传成功",
      "schema": {
        "type": "string",
        "title":"上传后的文件URL地址"
      }
    },
    {
      "id": "onFail",
      "title": "上传失败",
      "schema": {
        "type": "follow"
      }
    }
  ]
}