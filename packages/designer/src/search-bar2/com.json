{
  "title": "搜索框",
  "namespace": "mybricks.harmony.searchBar2",
  "author": "mybricks-team",
  "author_name": "板砖团队",
  "version": "1.0.0",
  "runtime": "./runtime.tsx",
  "data": "./data.json",
  "editors": "./editors.tsx",
  "ai": "./ai.ts",
  "icon": "./icon.svg",
  "inputs": [
    {
      "id": "setValue",
      "title": "设置值",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "getValue",
      "title": "获取值",
      "schema": {
        "type": "string"
      },
      "rels": ["returnValue"]
    },
    {
      "id": "focus",
      "title": "获取焦点",
      "schema": {
        "type": "any"
      },
      "rels" : ["focusDone"]
    }
  ],
  "outputs": [
    {
      "id": "onClick",
      "title": "当单击时",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "onChange",
      "title": "输入框内容变化时",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "onClear",
      "title": "点击清除按钮时",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "onCancel",
      "title": "当点击取消时",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "onSearch",
      "title": "当触发搜索时",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "returnValue",
      "title": "值输出",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "focusDone",
      "title": "获取焦点后",
      "schema": {
        "type": "any"
      }
    }
  ]
}
