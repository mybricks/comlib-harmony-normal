{
  "title": "数字旋钮",
  "namespace": "mybricks.harmony.formNumberKnob",
  "author": "mybricks-team",
  "author_name": "板砖团队",
  "version": "1.0.0",
  "runtime": "./runtime.tsx",
  "data": "./data.json",
  "editors": "./editors.tsx",
  "ai": "./ai.ts",
  "icon": "./icon.svg",
  "preview": "https://assets.mybricks.world/j864I8a0yIhYIY1XFNhGW5MqmAnSH1vR-1743580565071.png",
  "inputs": [
    {
      "id": "setValue",
      "title": "设置值",
      "desc": "设置表单项值",
      "schema": {
        "type": "number"
      },
      "rels": ["setValueComplete"]
    },
    {
      "id": "getValue",
      "title": "获取值",
      "desc": "获取表单项值并输出",
      "schema": {
        "type": "number"
      },
      "rels": ["returnValue"]
    },
    {
      "id": "setLabel",
      "title": "设置标题",
      "desc": "设置表单项标题，仅当表单项在表单容器内时生效",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "setDisabled",
      "title": "设置是否禁用",
      "desc": "设置表单项是否禁用，传入 true 时禁用表单项，传入 false 时启用表单项",
      "schema": {
        "type": "boolean"
      }
    }
  ],
  "outputs": [
    {
      "id": "onChange",
      "title": "当值变化",
      "schema": {
        "type": "number"
      }
    },
    {
      "id": "setValueComplete",
      "title": "设置值完成",
      "schema": {
        "type": "number"
      }
    },
    {
      "id": "returnValue",
      "title": "值输出",
      "schema": {
        "type": "number"
      }
    },
    {
      "id": "resetValueComplete",
      "title": "重置值完成",
      "schema": {
        "type": "number"
      }
    },
    {
      "id": "setDisabledComplete",
      "title": "是否禁用设置完成",
      "schema": {
        "type": "boolean"
      }
    }
  ],
  "schema": ["mybricks.taro.formContainer/formItem", "*"],
  "slots": []
}
