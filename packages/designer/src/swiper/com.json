{
  "title": "轮播",
  "namespace": "mybricks.harmony.swiper",
  "author": "mybricks-team",
  "author_name": "板砖团队",
  "version": "1.0.6",
  "runtime": "./runtime.tsx",
  "data": "./data.json",
  "editors": "./editors.tsx",
  "icon": "./icon.svg",
  "inputs": [
    {
      "id": "setItems",
      "title": "设置轮播内容",
      "schema": {
        "type": "array",
        "items": {
          "type": "object",
          "description":"thumbnail: 图片; customLink:点击后跳转的链接",
          "properties": {
            "thumbnail": {
              "title":"图片",
              "description":"图片",
              "type": "string"
            },
            "customLink": {
              "title":"点击后跳转链接",
              "description":"点击后跳转链接",
              "type": "string"
            }
          }
        }
      }
    }
  ],
  "outputs": [
    {
      "id": "onClick",
      "title": "当单击时",
      "schema": {
        "type": "object"
      }
    }
  ]
}
