          },
          "duration": {
            "title": "持续时间(ms)",
            "type": "number",
            "description": "持续时间，单位 ms"
          }
        }
      }
    }
  ],
  "outputs": [
    {
      "id": "afterShowToast",
      "title": "显示后",
      "schema": {
        "type": "follow"
      }
    }
